  "scripts": {
    "build": "gatsby build",
    "clean-cache": "gatsby clean",
    "dev": "npm run clean-cache && gatsby develop",
    "lint": "eslint .",
    "format": "prettier-eslint --write \"**/*.js\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "serve:functions": "netlify-lambda serve functions --config ./webpack.functions.js",
    "build:lambda": "netlify-lambda build functions --config ./webpack.functions.js && pwd && ls -la functions"
  },
  "devDependencies": {
    "@sanity/block-content-to-react": "^2.0.7",
